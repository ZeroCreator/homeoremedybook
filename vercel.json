{
  "version": 2,
  "builds": [{
    "src": "wsgi.py",
    "use": "@vercel/python",
    "config": {
      "maxLambdaSize": "15mb",
      "pythonVersion": "3.9"
    }
  }],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ]
}
